        <span class="operator">&lt;</span>Свойство Имя<span class="operator">=</span><span class="string">&quot;Кратность&quot;</span> Тип<span class="operator">=</span><span class="string">&quot;Число&quot;</span><span class="operator">&gt;</span>
            <span class="operator">&lt;</span>Значение<span class="operator">&gt;</span><span class="number">1</span><span class="operator">&lt;</span><span class="operator">/</span>Значение<span class="operator">&gt;</span>
        <span class="operator">&lt;</span><span class="operator">/</span>Свойство<span class="operator">&gt;</span>
