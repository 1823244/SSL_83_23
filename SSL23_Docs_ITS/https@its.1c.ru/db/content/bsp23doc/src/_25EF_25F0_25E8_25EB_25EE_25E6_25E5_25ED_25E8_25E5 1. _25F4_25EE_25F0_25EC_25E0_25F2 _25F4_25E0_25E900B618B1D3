<span class="operator">&lt;</span>Ссылка<span class="operator">&gt;</span>
            <span class="operator">&lt;</span>Свойство Имя<span class="operator">=</span><span class="string">&quot;Код&quot;</span> Тип<span class="operator">=</span><span class="string">&quot;Строка&quot;</span><span class="operator">&gt;</span>
                <span class="operator">&lt;</span>Значение<span class="operator">&gt;</span><span class="number">036</span><span class="operator">&lt;</span><span class="operator">/</span>Значение<span class="operator">&gt;</span>
