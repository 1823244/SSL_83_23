<script type="text/javascript">
	deferred.push(function(){
		var cookie = SSP.Bus.cookie('SEARCH_NEW');
		var a = SSP.DOM.byId('search_new_switch');
		
		if(!cookie || cookie === '1')
			a.innerHTML = 'Вернуться на старую версию поиска';
		else
			a.innerHTML = 'Попробовать новую версию поиска';
		
		var handler = function(event)
		{
			if(!cookie || cookie === '1')
				var newVersion = 0;
			else
				var newVersion = 1;
			
			SSP.Bus.cookie('SEARCH_NEW', newVersion);
			
			SSP.Bus.request('/db/search/versiontrack?new=' + newVersion.toString(), function(){
				window.location.reload();
			});
		};
		
		SSP.DOM.listen(a, 'click', handler);
	});
</script></ul><!--/noindex-->		</div>
